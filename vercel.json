{
  "version": 2,
  "buildCommand": "cd portfolio && npm install && npm run build",
  "outputDirectory": "portfolio/build",
  "framework": "create-react-app",
  "env": {
    "REACT_APP_API_URL": "@react_app_api_url"
  },
  "regions": ["sfo1"],
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/api/:path*",
      "destination": "https://api.example.com/:path*"
    }
  ]
}
